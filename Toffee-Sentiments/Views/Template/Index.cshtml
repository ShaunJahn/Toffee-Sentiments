@model TemplateModelView;

<h1 class="glow">Templates</h1>

<div class="container-fluid;" style="position: sticky;">
    <form asp-controller="Template" asp-action="GetTemplateName" method="post" role="form">
        <div class="border rounded-left border-primary " style="position: sticky;">
            <div class="row">
                <div class="col-sm-6 col-md-6 col-lg-6 row align-items-center justify-content-center">
                    <img class="mx-auto img-fluid" src="https://www.fineinvitepapers.com/image.asp?image=images/CS196-01.png&w=225" alt="" />
                    <div class="">
                        <input class="big" asp-for="getTemplate" type="radio" value="Square_Small" checked="checked" />   Small <br />
                        <input class="big" asp-for="getTemplate" type="radio" value="Square_Medium" checked="checked" />  Medium <br />
                        <input class="big" asp-for="getTemplate" type="radio" value="Square_Large" checked="checked" />   Large <br />
                    </div>
                </div>
                <div class="border-left rounded-left border-primary"></div>
                <div class="col-sm-6 col-md-6 col-lg-6 row align-items-center justify-content-center">
                    <img class="mx-auto img-fluid" src="https://d2jmvrsizmvf4x.cloudfront.net/INNT6RHNTaYMwuI1yWA4_rectangle.png" alt="" />
                    <div class="">
                        <input class="big" asp-for="getTemplate" type="radio" value="Rectangle_Small" checked="checked" />   Small <br />
                        <input class="big" asp-for="getTemplate" type="radio" value="Rectangle_Medium" checked="checked" />   Medium <br />
                        <input class="big" asp-for="getTemplate" type="radio" value="Rectangle_Large" checked="checked" />   Large <br />
                    </div>
                </div>
            </div>
        </div>
        <div class="border rounded-left border-primary">
            <div class="row">

                <div class="col-sm-6 col-md-6 col-lg-6 row align-items-center justify-content-center">
                    <img class="mx-auto img-fluid" src="https://i.pinimg.com/originals/80/81/cc/8081cca9354b7c4894f946367847a4f3.jpg" alt="" />
                    <div class="">
                        <input class="big" asp-for="getTemplate" type="radio" value="Tag_Small" checked="checked" />   Small <br />
                        <input class="big" asp-for="getTemplate" type="radio" value="Tag_Medium" checked="checked" />  Medium <br />
                        <input class="big" asp-for="getTemplate" type="radio" value="Tag_Large" checked="checked" />   Large <br />
                    </div>
                </div>

                <div class="border-left  rounded-left border-primary"></div>
                <div class="col-sm-6 col-md-6 col-lg-6 row align-items-center justify-content-center">
                    <img class="mx-auto img-fluid" src="data:image/jpeg;base64," alt="" />
                    <div class="">
                        <input class="big" asp-for="getTemplate" type="radio" value="Interactive_Shacker" checked="checked" />   Shacker <br />
                        <input class="big" asp-for="getTemplate" type="radio" value="Interactive_Slider" checked="checked" />   Slider <br />
                    </div>
                </div>
            </div>
        </div>
        <div class="col text-center">
            <input type="submit" class="btn btn-primary" value="Next" />
            </div>  
            <br />
            <br />
    </form>
</div>